{
  "name": "krenor/ldap-auth",
  "description": "Basic readonly authentication via LDAP for Laravel 5.5+",
  "keywords": [
    "laravel 5.5",
    "laravel",
    "laravel auth",
    "laravel authentication",
    "ldap",
    "ldap authentication",
    "active directory",
    "krenor"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Goldmann",
      "email": "stanislav.goldmann@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/krenor"
    }
  ],
  "require": {
    "illuminate/auth": "~5.5",
    "illuminate/contracts": "~5.5",
    "illuminate/support": "~5.5",
    "php": ">=7.0.0"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Krenor\\LdapAuth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Krenor\\LdapAuth\\LdapAuthServiceProvider"
      ]
    }
  }
}
